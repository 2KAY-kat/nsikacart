# =================================================================
# Local Development Settings (WAMP)
# =================================================================
RewriteEngine On
RewriteBase /nsikacart/public/

# Handle other routes
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ $1.html [L]

# Custom error pages
ErrorDocument 404 /nsikacart/404.md
ErrorDocument 500 /nsikacart/500.md